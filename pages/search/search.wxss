/* pages/search/search.wxss */
page {
    /* background: #F5F5F5; */
}

.search-wrap {
    /* position: fixed;
    top: 0;
    left: 0;
    right: 0; */
    display: flex;
    padding: 24rpx 30rpx;
    background-color: #fff;
    z-index: 99;
}

.search-wrap .search-back {
    width: 40rpx;
    height: 40rpx;
    margin-top: 7px;
}

.search-wrap .search {
    display: flex;
    flex: 1;
    /* margin-left: 15rpx; */
    padding: 16rpx 15rpx;
    padding-right: 30rpx;
    /* padding-left: 15rpx; */
    height: 40rpx;
    background: #F5F5F5;
    border-radius: 36rpx 36rpx 36rpx 36rpx;
}

.search-wrap .search .search-icon {
    margin-top: 9rpx;
    width: 28rpx;
    height: 28rpx;
    /* vertical-align: text-top; */
}

.search-wrap .search .search-input {
    flex: 1;
    display: inline-block;
    margin-left: 10rpx;
    font-size: 26rpx;
    /* vertical-align: middle; */
}

.search-wrap .search .search-btn {
    font-size: 28rpx;
    color: #0256FF;
}


/* result */
.result {}

.result .result-empty {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    width: 332rpx;
    height: 418rpx;
    text-align: center;
}

.result .result-empty image {
    width: 332rpx;
    height: 328rpx;
    margin-top: -50rpx;
    /* margin-bottom: 50rpx; */
}

.result .result-empty text {
    color: #AAAAAA;
    font-size: 36rpx;
}